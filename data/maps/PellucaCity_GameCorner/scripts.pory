    if (var(VAR_RESULT) == ITEM_LEAF_STONE) {setvar(VAR_0x8006, ITEM_LEAF_STONE) setvar(VAR_0x8002, PRICE_STONES) goto(PellucaCity_GameCorner_Shop2_BuyConfirm)}
